# =====================================
# Angular 20 .gitignore
# =====================================

# compiled output
/dist
/tmp
/out-tsc
/bazel-out
/build

# dependencies
/node_modules

# IDEs and editors
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# OS generated files
.DS_Store
Thumbs.db

# Angular specific
.angular/cache
.angular/local
.angular/tmp

# TypeScript
*.tsbuildinfo

# Environment files (if not intended for repo)
# uncomment below if you store secrets locally
# src/environments/environment.local.ts
# src/environments/environment.prod.local.ts
# src/environments/environment.dev.ts
# src/environments/environment.staging.ts

# Coverage reports
/coverage

# Compiled output (JS, maps, etc.)
*.js
*.map

# Misc
*.log
*.lock
*.bak
*.tmp
*.swp
*.swm

# Optional npm/yarn/pnpm lock files (use only one if your team standardizes)
# Uncomment the ones you DON'T use
# package-lock.json
# yarn.lock
# pnpm-lock.yaml

# Firebase / Hosting / Deployment
.firebase/
.firebase*
.firebaserc
/functions/lib/
.functions-cache/
hosting/.cache/

# VS Code local history / settings
.history/
*.code-workspace

# Temporary files
*.temp
*.tmp
~$*

# Environment specific local settings
.env
.env.*
!.env.example

# MacOS files
.AppleDouble
.LSOverride
Icon
ehthumbs.db

# Windows installer or binary files
*.exe
*.dll
*.msi
*.bat

# Linux / system
*~
.nfs*

# JetBrains
*.iml
*.ipr
*.iws

# Testing
/test-results
/junit.xml
/karma-*.xml
/lcov.info

# End of file
